# Market Data
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key_here

# Reddit API (for sentiment analysis)
REDDIT_CLIENT_ID=your_reddit_client_id
REDDIT_CLIENT_SECRET=your_reddit_client_secret
REDDIT_USER_AGENT=TradingAssistant/1.0

# News API
NEWS_API_KEY=your_newsapi_key_here

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/trading_assistant

# Redis Cache
REDIS_URL=redis://localhost:6379/0

# Apple Push Notifications
APNS_KEY_ID=your_apns_key_id
APNS_TEAM_ID=your_apns_team_id
APNS_KEY_PATH=./certs/apns_auth_key.p8
APNS_BUNDLE_ID=com.tradingassistant.app

# Flask
FLASK_SECRET_KEY=change-this-to-a-random-secret
FLASK_ENV=development
